using '../libraries/Map';

var Map test_map;
var ^MapNode test_node_pointer;

var boolean fr;

{   
    Map_add(@test_map,'first_key',5);
    Map_add(@test_map,'second_key',2);
    Map_add(@test_map,'third_key',5);
    Map_add(@test_map,'first_key',8);
    println('before delete');
    println(Map_get_value_by_key(@test_map,'first_key'));
    println(Map_get_value_by_key(@test_map,'second_key'));
    println(Map_get_value_by_key(@test_map,'third_key'));

    Map_delete_by_key(@test_map, 'second_key');

    println('after delete');
    println(Map_contains_by_key(@test_map,'first_key'));
    println(Map_contains_by_key(@test_map,'second_key'));
    println(Map_contains_by_key(@test_map,'third_key'));
}
